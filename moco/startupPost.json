[
  {
    "description": "post请求demo",
    "request": {
      "uri": "/postdemo",
      "method": "post"
    },
    "response": {
      "text": "这是一个不带请求参数的post请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "这是一个带参数的post请求",
    "request": {
      "uri": "/postwithparam",
      "method": "post",
      "forms": {
        "name": "huhansan",
        "age": "19"
      }
    },
    "response": {
      "text": "这是一个带参数的post请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]
